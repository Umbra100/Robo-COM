{
   "register": {
      "modal": {
         "trigger_id": null,
         "view": {
            "type": "modal",
            "title": {
               "type": "plain_text",
               "text": "User Registration"
            },
            "callback_id": "registerData",
            "blocks": [
               {
                  "type": "input",
                  "block_id": "nameField",
                  "label": {
                     "type": "plain_text",
                     "text": "Name"
                  },
                  "element": {
                     "type": "plain_text_input",
                     "action_id": "nameInput",
                     "multiline": false,
                     "placeholder": {
                        "type": "plain_text",
                        "text": "Billy Bob Joe"
                     },
                     "focus_on_load": true
                  },
                  "optional": false
               },
               {
                  "type": "input",
                  "block_id": "phoneField",
                  "label": {
                     "type": "plain_text",
                     "text": "Phone Number"
                  },
                  "element": {
                     "type": "plain_text_input",
                     "action_id": "phonenumberInput",
                     "multiline": false,
                     "placeholder": {
                        "type": "plain_text",
                        "text": "000-000-0000"
                     },
                     "max_length": 12
                  },
                  "optional": false
               },
               {
                  "type": "input",
                  "block_id": "teamField",
                  "label": {
                     "type": "plain_text",
                     "text": "What team are you a part of?"
                  },
                  "element": {
                     "type": "static_select",
                     "action_id": "teamInput",
                     "placeholder": {
                        "type": "plain_text",
                        "text": "Select"
                     },
                     "options": null
                  }
               },
               {
                  "type": "divider"
               },
               {
                  "type": "section",
                  "block_id": "notificationField",
                  "text": {
                     "type": "mrkdwn",
                     "text": "Would you like to recieve daily schedule reminders?"
                  },
                  "accessory": null
               }
            ],
            "submit": {
               "type": "plain_text",
               "text": "Submit"
            }
         }
      },
      "alternations": [
         {
            "options": {
               "yes": {
                  "type": "button",
                  "action_id": "notificationInput",
                  "text": {
                     "type": "plain_text",
                     "text": "Yes"
                  },
                  "style": "primary",
                  "confirm": {
                     "title": {
                        "type": "plain_text",
                        "text": "Are you sure?"
                     },
                     "text": {
                        "type": "plain_text",
                        "text": "You will not be able to recieve important notifications such as announcements through text message."
                     },
                     "confirm": {
                        "type": "plain_text",
                        "text": "Yes"
                     },
                     "deny": {
                        "type": "plain_text",
                        "text": "No"
                     }
                  }
               },
               "no": {
                  "type": "button",
                  "action_id": "notificationInput",
                  "text": {
                     "type": "plain_text",
                     "text": "No"
                  },
                  "style": "danger"
               }
            },
            "defaultOption": "yes"
         }
      ]
   },
   "notifyCommand": {
      "modal": {
         "trigger_id": null,
         "view": {
            "type": "modal",
            "title": {
               "type": "plain_text",
               "text": "Notify"
            },
            "blocks": [
               {
                  "type": "section",
                  "text": {
                     "type": "mrkdwn",
                     "text": "Do you want to notify everyone on the team?"
                  },
                  "block_id": "section01",
                  "accessory": {
                     "type": "button",
                     "text": {
                        "type": "plain_text",
                        "text": "Yes"
                     },
                     "action_id": "recipientButtonInput",
                     "style": "primary"
                  }
               },
               {
                  "type": "divider"
               },
               {
                  "type": "input",
                  "block_id": "messageField",
                  "label": {
                     "type": "plain_text",
                     "text": "Message"
                  },
                  "element": {
                     "type": "plain_text_input",
                     "action_id": "messageInput",
                     "placeholder": {
                        "type": "plain_text",
                        "text": "Write what you want to send in here."
                     },
                     "multiline": true
                  }
               }
            ],
            "callback_id": "notifyCommandData",
            "submit": {
               "type": "plain_text",
               "text": "Send"
            }
         }
      },
      "alternations": [
         {
            "yes": {
               "type": "button",
               "text": {
                  "type": "plain_text",
                  "text": "Yes"
               },
               "action_id": "recipientButtonInput",
               "style": "primary"
            },
            "no": {
               "type": "button",
               "text": {
                  "type": "plain_text",
                  "text": "No"
               },
               "action_id": "recipientButtonInput",
               "style": "danger"
            }
         },
         {
            "recipientChoice": {
               "type": "input",
               "block_id": "recipientField",
               "label": {
                  "type": "plain_text",
                  "text": "Recipients"
               },
               "element": {
                  "type": "multi_static_select",
                  "action_id": "recipientInput",
                  "placeholder": {
                     "type": "plain_text",
                     "text": "Teams and/or Users"
                  },
                  "options": null
               }
            }
         }
      ]
   },
   "notifyShortcut": {
      "modal": {
         "trigger_id": null,
         "view": {
            "type": "modal",
            "title": {
               "type": "plain_text",
               "text": "Notify"
            },
            "blocks": [
               {
                  "type": "section",
                  "text": {
                     "type": "mrkdwn",
                     "text": "Do you want to notify everyone on the team?"
                  },
                  "block_id": "section01",
                  "accessory": {
                     "type": "button",
                     "text": {
                        "type": "plain_text",
                        "text": "Yes"
                     },
                     "action_id": "recipientButtonInput",
                     "style": "primary"
                  }
               },
               {
                  "type": "divider"
               },
               {
                  "type": "input",
                  "block_id": "messageField",
                  "label": {
                     "type": "plain_text",
                     "text": "Message"
                  },
                  "element": {
                     "type": "plain_text_input",
                     "action_id": "messageInput",
                     "placeholder": {
                        "type": "plain_text",
                        "text": "Write what you want to send in here."
                     },
                     "multiline": true
                  },
                  "hint": {
                     "type": "plain_text",
                     "text": "Any files attached to this message will not be sent."
                  }
               }
            ],
            "callback_id": "notifyShortcutData",
            "submit": {
               "type": "plain_text",
               "text": "Send"
            }
         }
      }
   },
   "shutdown": {
      "modal": {
         "trigger_id": null,
         "view": {
            "type": "modal",
            "title": {
               "type": "plain_text",
               "text": "Authentication"
            },
            "blocks": [
               {
                  "type": "input",
                  "block_id": "passwordInput",
                  "label": {
                     "type": "plain_text",
                     "text": "Enter Password"
                  },
                  "element": {
                     "type": "plain_text_input",
                     "action_id": "passwordData",
                     "placeholder": {
                        "type": "plain_text",
                        "text": "Password"
                     },
                     "multiline": false
                  }
               }
            ],
            "callback_id": "shutdownData",
            "submit": {
               "type": "plain_text",
               "text": "Shutdown"
            }
         }
      }
   },
   "notebook": {
      "modal": {
         "trigger_id": null,
         "view": {
            "type": "modal",
            "title": {
               "type": "plain_text",
               "text": "Engineering Notebook"
            },
            "blocks": [
               {
                  "type": "header",
                  "text": {
                     "type": "plain_text",
                     "text": "What do you want to do?"
                  },
                  "block_id": "notebookHeader"
               },
               {
                  "type": "divider"
               },
               {
                  "type": "section",
                  "text": {
                     "type": "plain_text",
                     "text": "Make A New Entry"
                  },
                  "block_id": "section02",
                  "accessory": null
               },
               {
                  "type": "section",
                  "text": {
                     "type": "plain_text",
                     "text": "Look At Previous Entries"
                  },
                  "block_id": "section03",
                  "accessory": null
               },
               {
                  "type": "actions",
                  "block_id": "dateField",
                  "elements": [
                     {
                        "type": "datepicker",
                        "action_id": "notebookLogDatePick",
                        "initial_date": null
                     }
                  ]
               },
               {
                  "type": "divider"
               }
            ],
            "callback_id": "notebookData",
            "submit": {
               "type": "plain_text",
               "text": "Submit"
            }
         }
      },
      "alternations": [
         {
            "options": {
               "toggleOn": {
                  "type": "button",
                  "text": {
                     "type": "plain_text",
                     "text": " "
                  },
                  "action_id": null,
                  "style": "primary",
                  "value": "On"
               },
               "toggleOff": {
                  "type": "button",
                  "text": {
                     "type": "plain_text",
                     "text": " "
                  },
                  "action_id": null,
                  "value": "Off",
                  "style": "danger"
               }
            }
         },
         {
            "options": {
               "makeEntry": [
                  {
                     "type": "input",
                     "label": {
                        "type": "plain_text",
                        "text": "Title"
                     },
                     "block_id": "notebookTitleField",
                     "element": {
                        "type": "plain_text_input",
                        "action_id": "notebookTitleInput",
                        "max_length": 100,
                        "focus_on_load": true,
                        "placeholder": {
                           "type": "plain_text",
                           "text": "We did a thing!"
                        }
                     }
                  },
                  {
                     "type": "input",
                     "label": {
                        "type": "plain_text",
                        "text": "Description"
                     },
                     "block_id": "notebookDescriptionField",
                     "element": {
                        "type": "plain_text_input",
                        "action_id": "notebookDescriptionInput",
                        "placeholder": {
                           "type": "plain_text",
                           "text": "We did a thing by doing..."
                        },
                        "multiline": true
                     },
                     "optional": true
                  },
                  {
                     "type": "input",
                     "block_id": "userField",
                     "label": {
                        "type": "plain_text",
                        "text": " "
                     },
                     "element": {
                        "type": "multi_static_select",
                        "action_id": "userInput",
                        "options": null,
                        "placeholder": {
                           "type": "plain_text",
                           "text": "People That Attended"
                        }
                     }
                  },
                  {
                     "type": "actions",
                     "block_id": "fileURLFieldButton",
                     "elements": [
                        {
                           "type": "button",
                           "text": {
                              "type": "plain_text",
                              "text": "Add File URL"
                           },
                           "action_id": "fileURLAdd"
                        }
                     ]
                  }
               ],
               "viewEntry": [
                  {
                     "type": "header",
                     "block_id": "notebookViewHeader",
                     "text": {
                        "type": "plain_text",
                        "text": "Test!"
                     }
                  }
               ],
               "overwriteConfirm": {
                  "title": {
                     "type": "plain_text",
                     "text": "Are you sure?"
                  },
                  "text": {
                     "type": "mrkdwn",
                     "text": "A engineering note already exists on this date. Are you sure you want to overwrite it?"
                  },
                  "confirm": {
                     "type": "plain_text",
                     "text": "Yes"
                  },
                  "deny": {
                     "type": "plain_text",
                     "text": "No, go back!"
                  }
               },
               "enterFileURL": {
                  "type": "input",
                  "block_id": "fileURLField0",
                  "label": {
                     "type": "plain_text",
                     "text": " "
                  },
                  "element": {
                     "type": "plain_text_input",
                     "action_id": "fileURLInput0",
                     "placeholder": {
                        "type": "plain_text",
                        "text": "https://www.example.com/robot.jpg"
                     }
                  }
               },
               "removeFileURLButton": {
                  "type": "button",
                  "text": {
                     "type": "plain_text",
                     "text": "Remove Last"
                  },
                  "action_id": "fileURLRemove",
                  "style": "danger"
               }
            }
         }
      ]
   }
}
